{
    "name": "dansan/jobboy-driver-doctrine",
    "description": "Doctrine Driver for dansan/jobboy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Sanchi",
            "email": "danilo.sanchi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "dansan/jobboy": "^0.4.0"
    },
    "require-dev": {
        "dansan/jobboy-test-pack":"*",
        "doctrine/dbal": "~2.4",
        "dansan/fixture-handler": "*",
        "doctrine/migrations": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 60,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JobBoy\\": "src/JobBoy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-0.0": "0.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },

    "minimum-stability": "stable"
}
